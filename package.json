        "check:links-ci": "bash ./___scripts___/link-check/checklinksCI.sh",
        "ga:prod": "cp -r ./googleAnalytics/ga.prod.js ./src/ga.js",
        "ga:dev": "cp -r ./googleAnalytics/ga.dev.js ./src/ga.js",
