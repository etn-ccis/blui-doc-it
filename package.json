        "lint:fix": "eslint \"src/**/**.{tsx,ts}\" --fix",
        "prettier": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html,mdx}\" --write",
        "prettier:check": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html,mdx}\" --check",
        "cy:run": "cypress run",
        "cypress:open": "cypress open",
        "cytest": "start-server-and-test start http://localhost:3000 cy:run",
        "indexer": "cd scripts/crawl && node crawler.js && node indexer.js && cd ../.. && yarn prettier"
    },
